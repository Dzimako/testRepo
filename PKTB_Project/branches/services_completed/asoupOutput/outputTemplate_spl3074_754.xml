<reportOutputTemplate>
    <title>Выходной отчет</title>

    <parameters>
        <parameter>
            <key>rwCodeName</key>
            <label>Дорога:</label>
        </parameter>
        <parameter>
            <key>period</key>
            <label>Период запроса:</label>
        </parameter>
        <parameter>
            <key>razlozhenie</key>
            <label>Разложение:</label>
        </parameter>
        <parameter>
            <key>operation</key>
            <label>Объект операции:</label>
        </parameter>
        <parameter>
            <key>oper</key>
            <label>Операция:</label>
        </parameter>
        <parameter>
            <key>vidT</key>
            <label>Вид тяги:</label>
        </parameter>
        <parameter>
            <key>rodS</key>
            <label>Род службы серий:</label>
        </parameter>
    </parameters>

    <tables>
        <table>
            <headers>
                <rows>
                    <row>
                        <headerCells>
                            <headerCell>
                                <rowspan>2</rowspan>
                                <colspan>1</colspan>
                                <headerText>Операция</headerText>
                                <headerKey></headerKey>
                            </headerCell>
                            <headerCell>
                                <rowspan>2</rowspan>
                                <colspan>1</colspan>
                                <headerText>Дата и время операции</headerText>
                                <headerKey></headerKey>
                            </headerCell>
                            <headerCell>
                                <rowspan>2</rowspan>
                                <colspan>1</colspan>
                                <headerText>Состояние</headerText>
                                <headerKey></headerKey>
                            </headerCell>
                            <headerCell>
                                <rowspan>2</rowspan>
                                <colspan>1</colspan>
                                <headerText>Номер поезда</headerText>
                                <headerKey></headerKey>
                            </headerCell>
                            <headerCell>
                                <rowspan>2</rowspan>
                                <colspan>1</colspan>
                                <headerText>Индекс поезда</headerText>
                                <headerKey></headerKey>
                            </headerCell>
                            <headerCell>
                                <rowspan>2</rowspan>
                                <colspan>1</colspan>
                                <headerText>Дислокация</headerText>
                                <headerKey></headerKey>
                            </headerCell>
                            <headerCell>
                                <rowspan>1</rowspan>
                                <colspan>3</colspan>
                                <headerText>Данные о бюджете времени</headerText>
                                <headerKey></headerKey>
                            </headerCell>
                        </headerCells>
                    </row>
                    <row>
                        <headerCells>
                            <headerCell>
                                <rowspan>1</rowspan>
                                <colspan>1</colspan>
                                <headerText>Учетный вид движения (работы)</headerText>
                                <headerKey></headerKey>
                            </headerCell>
                            <headerCell>
                                <rowspan>1</rowspan>
                                <colspan>1</colspan>
                                <headerText>Учетная категория (код, наим.)</headerText>
                                <headerKey></headerKey>
                            </headerCell>
                            <headerCell>
                                <rowspan>1</rowspan>
                                <colspan>1</colspan>
                                <headerText>Бюджет времени (час. мин.)</headerText>
                                <headerKey></headerKey>
                            </headerCell>
                        </headerCells>
                    </row>
                </rows>
            </headers>
        </table>
    </tables>
</reportOutputTemplate>